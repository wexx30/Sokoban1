#include <iostream>

using namespace std;

int main (){
    float a , b;
    cout << "Pleae enter a and b:"<< endl;
    try{
        cin >> a >> b;
        if(b==0)
            throw b;
        else
            cout << "a/b:" << a/b << endl;

    }
    catch(...){
        cout << "error" << endl;
    }

    return 0;
}
