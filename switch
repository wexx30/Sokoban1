#include <iostream>

using namespace std;

int main (){
    float score;
    cout << "Please enter your score" << endl;
    cin >> score;

    if(score <= 100 && score >= 0){
        int grade = score / 10;
        switch(grade){
            case 10 :
                cout << "A" << endl;
                break;
            case 9 :
                cout << "B" << endl;
                break;
            case 8 :
                cout << "C" << endl;
                break;
            case 7 :
                cout << "D" << endl;
                break;
            case 6 :
                cout << "F" << endl;
                break;
            default:
                cout << "G" << endl;
                break;

        }
    }
    else
        cout << "Error" << endl;
    return 0;
}
